{
  "name": "eleventy-netlify-boilerplate",
  "version": "1.0.3",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "clean": "rimraf _site",
    "build": "npx eleventy",
    "watch": "npx eleventy --watch",
    "dev": "npx eleventy --serve",
    "serve:eleventy": "eleventy --serve --watch",
    "serve:mix": "mix watch",
    "serve": "npm-run-all clean --parallel serve:* --print-label",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate"
  },
  "author": {
    "name": "Dan Urbanowicz",
    "email": "dan.urbanowicz@gmail.com",
    "url": "https://www.danurbanowicz.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate/issues"
  },
  "homepage": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@savvywombat/tailwindcss-grid-areas": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "luxon": "^1.25.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "clean-css": "^4.2.1",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^6.0.41",
    "uglify-js": "^3.14.2"
  }
}
